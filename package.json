{
  "name": "unifly",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "start": "react-native start",
    "test": "jest",
    "asset": "react-native-asset",
    "pod": "cd ios && pod install && cd ..",
    "pod-update": "cd ios && pod update && cd ..",
    "release": "react-native build-android --mode=release",
    "apk": "cd android && ./gradlew assembleRelease",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "download-schema": "apollo client:download-schema --endpoint=https://unifly-api.arrakmit.com/graphql schema.graphql",
    "download-schema-local": "apollo client:download-schema --endpoint=http://localhost:5000/graphql schema.graphql",
    "codegen": "graphql-codegen --config codegen.ts",
    "connect-localhost": "adb reverse tcp:5000 tcp:5000"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
